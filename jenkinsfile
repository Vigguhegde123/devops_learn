pipeline {
    agent any 
    stages {
        steps('code analaysis') {
            steps{
            sh 'echo "sonar running"'
            }
        }
        stages('code tetsing'){
            steps{
            sh 'echo "testing"'
            }
        }
        stage('build'){
            steps{
            sh 'echo "building application"'
            }
        }
        stage('deploy'){
            steps{
            sh 'echo "application deployed to prod server"'
        }
    }
}
}
